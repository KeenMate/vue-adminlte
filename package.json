{
	"name": "@keenmate/vue-adminlte",
	"version": "0.1.17",
	"description": "",
	"author": {
		"email": "benjamin.horne@keenmate.com",
		"name": "Benjamin Horne"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KeenMate/vue-adminlte.git"
	},
	"main": "dist/vue-adminlte.umd.js",
	"module": "dist/vue-adminlte.esm.js",
	"unpkg": "dist/vue-adminlte.min.js",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"dev": "rollup -c --environment NODE_ENV:development",
		"dev.old": "vue serve --open src/app.vue",
		"build": "rollup -c --environment NODE_ENV:production",
		"build.old": "npm run build:umd & npm run build:es & npm run build:unpkg",
		"build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-adminlte.umd.js",
		"build:es": "rollup --config rollup.config.js --format es --file dist/vue-adminlte.esm.js",
		"build:unpkg": "rollup --config rollup.config.js --format iife --file dist/vue-adminlte.min.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"demo": "rollup -c rollup.config.demo.js --environment DEV",
		"demo:build": "rollup -c rollup.config.demo.js",
		"docs": "npm run docs:serve",
		"docs:build": "npm run demo:build && cd docs && vuepress build && cd .. && mv demo/dist docs/.vuepress/dist/demo",
		"docs:serve": "",
		"start": "vue-cli-service serve"
	},
	"dependencies": {
		"jquery": "^3.5.1",
		"vue-popperjs": "^2.3.0",
		"vue-router": "^3.1.6",
		"vuelidate": "^0.7.5",
		"vuelidate-error-extractor": "^2.4.1",
		"vuepress-plugin-sitemap": "^2.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.9.6",
		"@babel/plugin-transform-runtime": "^7.9.6",
		"@rollup/plugin-alias": "^3.1.0",
		"@rollup/plugin-babel": "^5.0.2",
		"@rollup/plugin-buble": "^0.21.3",
		"@rollup/plugin-commonjs": "^12.0.0",
		"@rollup/plugin-html": "^0.2.0",
		"@rollup/plugin-node-resolve": "^8.0.0",
		"@vue/cli-plugin-babel": "^4.3.1",
		"@vue/cli-service": "^4.3.1",
		"@vue/eslint-config-standard": "^5.1.2",
		"babel-eslint": "^10.1.0",
		"babel-preset-vue": "^2.0.2",
		"eslint": "^6.8.0",
		"eslint-plugin-compat": "^3.7.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"eslint-plugin-vue": "^6.2.2",
		"esm": "^3.2.25",
		"node-sass": "^4.14.1",
		"rollup": "^2.10.5",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-generate-html-template": "^1.6.1",
		"rollup-plugin-livereload": "^1.3.0",
		"rollup-plugin-postcss": "^3.1.1",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-styles": "^3.5.5",
		"rollup-plugin-terser": "^6.0.1",
		"rollup-plugin-vue": "^5.1.9",
		"vue": "^2.6.11",
		"vue-template-compiler": "^2.6.11"
	},
	"license": "MIT"
}
