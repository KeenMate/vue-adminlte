{
	"name": "@keenmate/vue-adminlte",
	"version": "0.1.17",
	"description": "",
	"author": {
		"email": "benjamin.horne@keenmate.com",
		"name": "Benjamin Horne"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KeenMate/vue-adminlte.git"
	},
	"main": "dist/minimal-vue-package.umd.js",
	"module": "dist/minimal-vue-package.esm.js",
	"unpkg": "dist/minimal-vue-package.min.js",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"dev": "vue serve --open src/app.vue",
		"build": "rollup -c",
		"build.old": "npm run build:umd & npm run build:es & npm run build:unpkg",
		"build:umd": "rollup --config rollup.config.js --format umd --file dist/minimal-vue-package.umd.js",
		"build:es": "rollup --config rollup.config.js --format es --file dist/minimal-vue-package.esm.js",
		"build:unpkg": "rollup --config rollup.config.js --format iife --file dist/minimal-vue-package.min.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"jquery": "^3.5.1",
		"vue-popperjs": "^2.3.0",
		"vue-router": "^3.1.6",
		"vuelidate": "^0.7.5",
		"vuelidate-error-extractor": "^2.4.1",
		"vuepress-plugin-sitemap": "^2.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.9.6",
		"@rollup/plugin-babel": "^5.0.2",
		"@rollup/plugin-buble": "^0.21.3",
		"@rollup/plugin-commonjs": "^12.0.0",
		"@rollup/plugin-node-resolve": "^8.0.0",
		"babel-eslint": "^10.1.0",
		"babel-preset-vue": "^2.0.2",
		"rollup": "^2.10.5",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-postcss": "^3.1.1",
		"rollup-plugin-styles": "^3.5.5",
		"rollup-plugin-terser": "^6.0.1",
		"rollup-plugin-vue": "^5.1.9",
		"vue": "^2.6.11",
		"vue-template-compiler": "^2.6.11"
	},
	"license": "MIT"
}
